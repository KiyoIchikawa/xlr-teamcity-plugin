<synthetic xsi:schemaLocation="http://www.xebialabs.com/deployit/synthetic synthetic.xsd" xmlns="http://www.xebialabs.com/deployit/synthetic" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    
    
    <type type="teamcity.Build" extends="xlrelease.PythonScript">
        <property name="scriptLocation" default="teamcity/Build.py" hidden="true" />
        <property name="teamcityServer" category="input" label="Server" referenced-type="teamcity.Server" kind="ci"/>
